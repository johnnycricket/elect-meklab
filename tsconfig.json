{
  "compilerOptions": {
    "target": "ESNext",
    "module": "commonjs",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "baseUrl": ".",
    "outDir": "dist",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "importHelpers": true,
    "resolveJsonModule": true,
    "strict": true,
    "jsx":"react-jsx"
  }
}
